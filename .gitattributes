# Count backend code as Java
backend/**/*.java linguist-detectable=true
backend/**/*.properties linguist-language=Java

# Count admin-frontend as JavaScript/React
admin-frontend/**/*.js linguist-language=JavaScript

# Count user-mobile as React Native (JavaScript)
user-mobile/**/*.js linguist-language=JavaScript

# Ignore auto-generated / build artifacts
**/node_modules/* linguist-vendored
**/build/* linguist-vendored
**/dist/* linguist-vendored
